{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "node_modules/better-docs/component"],
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md",
      "template": "node_modules/better-docs"
    },
    "templates": {
      "better-docs": {
        "name": "Sample Documentation"
      }
    }
  }